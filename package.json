{
  "name": "@dwidge/query-axios-zod",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/query.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc && tsc --project tsconfig.cjs.json",
    "clean": "rimraf dist",
    "test": "tsc --project tsconfig.test.json && node --enable-source-maps --test --test-name-pattern"
  },
  "peerDependencies": {
    "axios": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
