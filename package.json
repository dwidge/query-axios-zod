{
  "name": "@dwidge/query-axios-zod",
  "version": "0.0.16",
  "description": "A library for querying JSON data using axios and Zod.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/query-axios-zod.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc -b . tsconfig.cjs.json tsconfig.test.json",
    "test": "node --enable-source-maps --test --test-name-pattern"
  },
  "peerDependencies": {
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20"
  }
}
